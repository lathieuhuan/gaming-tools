    "@tailwindcss/vite": "4.1.13",
    "@types/node": "20.11.24",
    "@types/react": "18.2.56",
    "@types/react-dom": "18.2.19",
    "@vitejs/plugin-react": "5.0.3",
    "eslint-plugin-storybook": "9.1.9",
